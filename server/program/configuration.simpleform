FORM Configuration
{
	configuration
	{
		id @int
		form @string
		name string
		description string
		category {
			id @int
			_ string
		}
		comment string
		recipe {
			id @int
			_ string
		}
		component {
			id @int
			code string
			form string
			name string
			categoryName string
			manufacturerName string
			mfgCode string
			webPage string
			description string
			price float
		}
		search
	}
}
